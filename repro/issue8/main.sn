struct Builder =>
    value: int

    static fn new(): Builder =>
        var b: Builder = Builder { value: 0 }
        return b

    fn add(n: int): Builder =>
        self.value = self.value + n
        return self

    fn multiply(n: int): Builder =>
        self.value = self.value * n
        return self

fn main(): void =>
    var b: Builder = Builder.new()
        .add(5)
        .multiply(2)
    print($"{b.value}\n")
