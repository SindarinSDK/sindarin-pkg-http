# Reproduction: Static methods cannot access struct fields on local variables

struct Point =>
    x: int
    y: int

    static fn create(a: int, b: int): Point =>
        var p: Point = Point { x: 0, y: 0 }
        p.x = a
        p.y = b
        return p

fn main(): void =>
    var p: Point = Point.create(10, 20)
    print($"x={p.x}, y={p.y}\n")
