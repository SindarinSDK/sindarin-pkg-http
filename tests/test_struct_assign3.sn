# Test struct field assignment in regular function

struct Point =>
    x: int
    y: int

fn createPoint(a: int, b: int): Point =>
    var p: Point = Point { x: 0, y: 0 }
    p.x = a
    p.y = b
    return p

fn main(): void =>
    var p: Point = createPoint(10, 20)
    print($"x={p.x}, y={p.y}\n")
